<h2>It's time to work! </h2>
<h3>Take a look at our contributors'requests </h2>
<ul>
  <% organizations.forEach((organization) => {%>
    <li><a href="/organizations/<%=organization.id%>"><%=organization.name%></a>
      <%=organization.description%>
      <%=organization.contacts%>
      <%=organization.address%>
      <%=organization.category%>
      <%=organization.status%>
      <%=organization.ownerId%>
      <form class="" action="/organizations/<%=organization.id%>/delete" method="POST">
      <input type="submit"class="button" name="button" value="Delete Organization">
      </form>
    </li>
<%  })%>
</ul>
<a class= "button is-success is-normal" href="/celebrities/new">Add new organization</a>
